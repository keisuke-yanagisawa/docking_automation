{
    "runArgs": [],
    "name": "docking_automation",
    "dockerFile": "Dockerfile",
    "extensions": [
        "github.copilot",
        "ms-python.python",
        "ms-toolsai.jupyter",
        "mhutchie.git-graph"
    ],
    "mounts": [
        "source=${localEnv:HOME}${localEnv:USERPROFILE}/.ssh,target=/root/.ssh,type=bind",
        "source=${localEnv:HOME}${localEnv:USERPROFILE}/.gitconfig,target=/root/.gitconfig,type=bind"
    ]
}
